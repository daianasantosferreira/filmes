1-SELECT * FROM `ENTRETERIMENTO` WHERE TIPO = "série";

2-SELECT * 
FROM ENTRETERIMENTO 
WHERE TIPO = 'série' 
ORDER BY NOME ASC;

3-SELECT *
FROM ENTRETERIMENTO
WHERE TIPO = 'série'
ORDER BY NOME DESC;

4-SELECT *
FROM ENTRETERIMENTO
WHERE TIPO = 'FILME';

5-SELECT * FROM ENTRETENIMENTO WHERE TIPO = 'FILME' AND ANO_LANCAMENTO < 2000; 

6-SELECT * FROM ENTRETERIMENTO WHERE tipo = 'série' AND ANO_PRODUCAO > 2015;

7-SELECT * FROM ENTRETERIMENTO WHERE GENERO LIKE 'Ação%' OR GENERO LIKE 'Ação/Drama'; 

8-SELECT * FROM ENTRETERIMENTO WHERE TIPO = 'FILME' AND DURACAO_MINUTOS > 120;

9-SELECT * FROM ENTRETERIMENTO WHERE TIPO = 'série' AND ANO_LANCAMENTO BETWEEN 2012 AND 2016 ORDER BY ANO_LANCAMENTO, NOME; 

10-SELECT * FROM ENTRETERIMENTO WHERE TIPO = 'FILME' AND ATOR_PRINCIPAL LIKE '%Brad Pitt%' ORDER BY ANO_LANCAMENTO ASC;

11-SELECT NOME, DIRETOR, ATOR_PRINCIPAL FROM ENTRETERIMENTO WHERE GENERO LIKE 'Crime%' AND ATOR_PRINCIPAL LIKE '%John Travolta%';
 
12-SELECT * FROM ENTRETERIMENTO WHERE DURACAO_MINUTOS > 120 AND ATOR_PRINCIPAL NOT LIKE '%Christian Bale%' AND ATOR_PRINCIPAL NOT LIKE '%Tim Robbins%';

13-SELECT * FROM ENTRETERIMENTO WHERE GENERO NOT LIKE 'Comédia%' AND GENERO NOT LIKE 'Drama%'; 

14-SELECT * FROM ENTRETERIMENTO WHERE ANO_LANCAMENTO NOT IN (2000, 2001, 2002); 

15-SELECT * FROM ENTRETERIMENTO WHERE ATOR_PRINCIPAL LIKE '%Wagner Moura%' OR ATOR_PRINCIPAL LIKE '%Leonardo DiCaprio%' OR ATOR_PRINCIPAL LIKE '%Keanu Reeves%';
